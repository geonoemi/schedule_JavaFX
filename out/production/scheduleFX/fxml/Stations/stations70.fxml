<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox stylesheets="/css/main.css"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="sample.Controllers.stations70"
      fx:id="container">

    <children>

        <Label fx:id="title" text="Choose Station:"/>

        <HBox styleClass="busButtons">
            <children>
                <Button fx:id="stat1" text="Füvészkert" onAction="#stat1Click"/>
                <Button fx:id="stat2" text="Mars-tér" onAction="#stat2Click" />
                <Button fx:id="stat3" text="Széchenyi-tér" onAction="#stat3Click"/>
            </children>

        </HBox>

    </children>
</VBox>
